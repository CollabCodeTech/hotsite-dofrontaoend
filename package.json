{
  "name": "hotsite-dofrontaoend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "5.0.1",
    "react-scroll": "^1.7.12",
    "styled-components": "^4.2.0"
  },
  "scripts": {
    "prettier:all": "prettier --write src/**/*.js",
    "lint": "eslint src/**/*.js",
    "lint:fix": "npm run lint -- --fix",
    "precommit": "cross-env NODE_ENV=production lint-staged",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7",
    "prettier": "^1.17.1",
    "pretty-quick": "^1.11.0"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "pretty-quick --staged",
      "npm run lint:fix",
      "git add"
    ]
  }
}
